javascript:void(function(){var links=document.querySelectorAll('a');var colors={link:'#A7D189',nofollow:'#FDBD76',ugc:'#FAEBA6',sponsored:'#EB949D'};var container=document.createElement('div');container.style.position='fixed';container.style.bottom='0';container.style.right='0';container.style.backgroundColor='white';container.style.width='300px';container.style.zIndex='9999';var header=document.createElement('div');header.innerHTML='Link Types';container.appendChild(header);var featureBox=document.createElement('div');featureBox.style.border='1px solid grey';container.appendChild(featureBox);var closeButton=document.createElement('span');closeButton.innerText='X';closeButton.style.position='absolute';closeButton.style.top='0';closeButton.style.right='5px';closeButton.style.cursor='pointer';closeButton.addEventListener('click',function(){container.remove();});container.appendChild(closeButton);links.forEach(function(link){var relAttr=link.getAttribute('rel');if(relAttr){var rels=relAttr.split(' ').map(function(rel){return rel.toLowerCase().trim();});if(rels.includes('nofollow')){link.style.backgroundColor=colors.nofollow;}else if(rels.includes('ugc')){link.style.backgroundColor=colors.ugc;}else if(rels.includes('sponsored')){link.style.backgroundColor=colors.sponsored;}}else{link.style.backgroundColor=colors.link;}link.style.color='black';link.style.border='1px solid grey';link.style.padding='3px';});for(var key in colors){var colorBox=document.createElement('div');colorBox.style.backgroundColor=colors[key];colorBox.style.width='100%';colorBox.style.height='20px';colorBox.style.margin='5px 0';colorBox.style.lineHeight='20px';colorBox.style.textAlign='center';colorBox.innerText=key;featureBox.appendChild(colorBox);}var metaRobots=document.querySelector('meta[name="robots"]');var indexable='Yes';if(metaRobots&&metaRobots.getAttribute('content').includes('noindex')){indexable='No';}var indexDiv=document.createElement('div');indexDiv.innerHTML='Indexable: '+indexable;featureBox.appendChild(indexDiv);var creditDiv=document.createElement('div');creditDiv.innerHTML='Made with Love ‚ù§ using ChatGPT by Ali Silat';creditDiv.style.fontSize='12px';container.appendChild(creditDiv);document.body.appendChild(container);})();
